CREATE USER VOkebaso IDENTIFIED BY  strongpassword DEFAULT TABLESPACE "IC_STAGE_TEMP"
      TEMPORARY TABLESPACE "USERSTEMP"
      PROFILE "INDIVIDUAL_ACCOUNTS";
 
   GRANT "BIP_ROLE" TO VOkebaso;
 
  GRANT CREATE TABLE TO VOkebaso;
  GRANT CREATE SESSION TO VOkebaso;
  alter user VOkebaso quota 50g on IC_STAGE_TEMP;
  commit;



